using System;

int fibonacci(int n)
{
    if (n == 0) return 0;
    if (n == 1) return 1;
    return fibonacci(n - 2) + fibonacci(n - 1);
}

void main()
{
    Console.WriteLine("First ten Fibonacci numbers are:");
    for (int n = 0; n < 10; ++n)
    {
        Console.Out() << "fibonacci(" << n << ") == " << fibonacci(n) << endl();
    }
}
