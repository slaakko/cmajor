using System;
using System.Collections;

//  Writes:
//  7, 6, 4, 4, 0, -1

void main()
{
    List<int> list;
    list.Add(7);
    list.Add(-1);
    list.Add(4);
    list.Add(6);
    list.Add(0);
    list.Add(4);
    InsertionSort(list.Begin(), list.End(), Greater<int>());
    Console.Out() << list << endl();
}
